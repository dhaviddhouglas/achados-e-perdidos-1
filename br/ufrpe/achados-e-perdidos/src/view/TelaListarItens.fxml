<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.TelaListarItensController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../plano%20de%20fundo%20das%20telas.png" />
         </image>
      </ImageView>
      <Text fill="WHITE" layoutX="257.0" layoutY="68.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Itens Cadastrados:">
         <font>
            <Font name="Agency FB" size="40.0" />
         </font>
      </Text>
      <Button fx:id="btVoltar" layoutX="38.0" layoutY="525.0" mnemonicParsing="false" prefHeight="11.0" prefWidth="95.0" text="Voltar">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <TableView fx:id="tbView" layoutX="59.0" layoutY="86.0" prefHeight="410.0" prefWidth="683.0">
        <columns>
          <TableColumn fx:id="tcCat" prefWidth="95.0" text="Categoria" />
          <TableColumn fx:id="tcID" prefWidth="55.0" text="ID" />
            <TableColumn fx:id="tcTipo" prefWidth="98.0" text="Tipo" />
            <TableColumn fx:id="tcCor" prefWidth="94.0" text="Cor" />
            <TableColumn fx:id="tcQtd" prefWidth="44.0" text="Qtd." />
            <TableColumn fx:id="tcDescricao" prefWidth="294.0" text="Descrição" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
